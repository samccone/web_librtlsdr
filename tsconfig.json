{
  "compilerOptions": {
    "module": "amd",
    "target": "es6",
    "outFile": "main.js",
    "noImplicitAny": true,
    "inlineSourceMap": true,
    "noEmitOnError": false,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "lib": ["es2017", "dom"]
  },
  "include": ["*.ts"]
}
